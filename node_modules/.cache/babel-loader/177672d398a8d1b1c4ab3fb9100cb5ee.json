>\n        <div>\n          <BtnMenu msg=\"Gerar endereço\" v-on:click=\"getEnd()\" />\n        </div>\n      </div>\n    </div>\n    <div\n      class=\"card-down\"\n      :id=\"endereco.siafi\"\n      v-for=\"endereco in enderecos\"\n      :key=\"endereco.siafi\"\n    >\n      <CardDash\n        :logradouro=\"endereco.logradouro\"\n        :cep=\"endereco.cep\"\n        :localidade=\"endereco.localidade\"\n        :uf=\"endereco.uf\"\n        v-on:click=\"removeCep(endereco, endereco.cep)\"\n      />\n    </div>\n  </section>\n</template>\n<script>\nimport BtnMenu from \"./BtnMenu.vue\";\nimport axios from \"axios\";\nimport CardCep from \"./CardCep.vue\";\nimport CardDash from \"./CardDash.vue\";\nexport default {\n  name: \"DashboardMenu\",\n  components: {\n    BtnMenu,\n    CardCep,\n    CardDash,\n  },\n  data() {\n    return {\n      cep: null,\n      ceps: [],\n      enderecos: [],\n      urls: [],\n      baseUrl: \"https://viacep.com.br/ws/\",\n    };\n  },\n  methods: {\n    getEnd() {\n      for (this.cep of this.ceps) {\n        var url = `${this.baseUrl}${this.cep}/json/`;\n        if (!this.urls.includes(url)) {\n          this.urls.push(url);\n          axios\n            .get(url)\n            .then((res) => {\n              this.enderecos.push(res.data);\n            })\n            .catch((error) => {\n              console.log(error);\n            });\n        }\n      }\n\n      console.log(this.enderecos);\n    },\n\n    insertCep() {\n      if (!this.ceps.includes(this.cep) && this.cep.length > 7) {\n        this.ceps.push(this.cep);\n      }\n\n      console.log(this.ceps);\n    },\n\n    removeCep(id, cep) {\n      var indexEnd = this.enderecos.indexOf(id);\n      var indexCep = this.ceps.includes(cep);\n      console.log(indexEnd)\n      console.log(indexCep)\n      this.ceps.splice(this.ceps.indexOf(cep), 1);\n      this.enderecos.splice(indexEnd, 1);\n    },\n  },\n};\n</script>\n<style scoped>\n.card-dash {\n  margin: 30px 135px;\n  min-height: 400px;\n  width: 540px;\n}\n\n.div-cep {\n  margin-bottom: 15px;\n}\n\n.card-top {\n  min-height: 220px;\n  width: 100%;\n  padding-bottom: 20px;\n  border-bottom: 1px solid #b7b7b7;\n}\n\n.top-div-top {\n  height: 60px;\n  width: 100%;\n  display: flex;\n  justify-content: space-between;\n}\n\n.top-div-middle {\n  min-height: 70px;\n  margin-top: 25px;\n  margin-bottom: 40px;\n  width: 100%;\n}\n\n.top-div-down {\n  width: 100%;\n  justify-content: end;\n  align-content: center;\n  display: flex;\n}\n\n.input-cep {\n  min-width: 250px;\n  height: 45px;\n  border: 2px solid #bbc4ce;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: flex-start;\n  padding: 16px;\n  box-sizing: border-box;\n  border-radius: 10px;\n}\n\n::-webkit-input-placeholder {\n  font-family: \"Open Sans\";\n  font-weight: 400;\n  size: 16px;\n}\n\n:-ms-input-placeholder {\n  font-family: \"Open Sans\";\n  font-weight: 400;\n  size: 16px;\n}\n</style>"]},"metadata":{},"sourceType":"module"}