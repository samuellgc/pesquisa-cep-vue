{"ast":null,"code":"import \"core-js/modules/es.array.includes.js\";\nimport BtnMenu from \"./BtnMenu.vue\";\nimport axios from \"axios\";\nimport CardCep from \"./CardCep.vue\";\nimport CardDash from \"./CardDash.vue\";\nexport default {\n  name: \"DashboardMenu\",\n  components: {\n    BtnMenu,\n    CardCep,\n    CardDash\n  },\n\n  data() {\n    return {\n      cep: null,\n      ceps: [],\n      enderecos: [],\n      urls: [],\n      baseUrl: \"https://viacep.com.br/ws/\"\n    };\n  },\n\n  methods: {\n    getEnd() {\n      for (this.cep of this.ceps) {\n        var url = `${this.baseUrl}${this.cep}/json/`;\n\n        if (!this.urls.includes(url)) {\n          this.urls.push(url);\n          axios.get(url).then(res => {\n            this.enderecos.push(res.data);\n          }).catch(error => {\n            console.log(error);\n          });\n        }\n      }\n\n      console.log(this.enderecos);\n    },\n\n    insertCep() {\n      if (!this.ceps.includes(this.cep) && this.cep.length > 7) {\n        this.ceps.push(this.cep);\n      }\n\n      console.log(this.ceps);\n    },\n\n    removeCep(id, cep) {\n      var indexEnd = this.enderecos.indexOf(id);\n      var indexCep = this.ceps.includes(cep);\n      console.log(indexEnd);\n      console.log(indexCep);\n      this.ceps.splice(this.ceps.indexOf(cep), 1);\n      this.enderecos.splice(indexEnd, 1);\n    }\n\n  }\n};","map":{"version":3,"mappings":";AA8CA,OAAOA,OAAP,MAAoB,eAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,OAAP,MAAoB,eAApB;AACA,OAAOC,QAAP,MAAqB,gBAArB;AACA,eAAe;EACbC,IAAI,EAAE,eADO;EAEbC,UAAU,EAAE;IACVL,OADU;IAEVE,OAFU;IAGVC;EAHU,CAFC;;EAObG,IAAI,GAAG;IACL,OAAO;MACLC,GAAG,EAAE,IADA;MAELC,IAAI,EAAE,EAFD;MAGLC,SAAS,EAAE,EAHN;MAILC,IAAI,EAAE,EAJD;MAKLC,OAAO,EAAE;IALJ,CAAP;EAOD,CAfY;;EAgBbC,OAAO,EAAE;IACPC,MAAM,GAAG;MACP,KAAK,KAAKN,GAAV,IAAiB,KAAKC,IAAtB,EAA4B;QAC1B,IAAIM,GAAE,GAAK,GAAE,KAAKH,OAAQ,GAAE,KAAKJ,GAAI,QAArC;;QACA,IAAI,CAAC,KAAKG,IAAL,CAAUK,QAAV,CAAmBD,GAAnB,CAAL,EAA8B;UAC5B,KAAKJ,IAAL,CAAUM,IAAV