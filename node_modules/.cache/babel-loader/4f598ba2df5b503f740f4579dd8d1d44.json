{"ast":null,"code":"import BtnMenu from \"./BtnMenu.vue\";\nimport axios from \"axios\";\nimport CardCep from \"./CardCep.vue\";\nimport CardDash from \"./CardDash.vue\";\nexport default {\n  name: \"DashboardMenu\",\n  components: {\n    BtnMenu,\n    CardCep,\n    CardDash\n  },\n\n  data() {\n    return {\n      info: null,\n      ceps: [],\n      enderecos: [],\n      baseUrl: \"https://viacep.com.br/ws/\"\n    };\n  },\n\n  methods: {\n    getEnd() {\n      for (this.cep of this.ceps) {\n        var url = `${this.baseUrl}${this.cep}/json/`;\n      }\n\n      axios.get(url).then(res => {\n        this.enderecos.push(res.data);\n        console.log(this.enderecos);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    limparInput() {\n      document.getElementById(\"cep\").value = \"\";\n    },\n\n    insertCep() {\n      this.ceps.push(this.info);\n      console.log(this.ceps);\n      this.limparInput();\n    },\n\n    removeCep() {\n      for (this.cep in this.ceps) {\n        this.ceps.splice(this.cep, 1);\n      }\n\n      for (this.endereco in this.enderecos) {\n        this.enderecos.splice(this.endereco, 1);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8CA,OAAOA,OAAP,MAAoB,eAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,OAAP,MAAoB,eAApB;AACA,OAAOC,QAAP,MAAqB,gBAArB;AACA,eAAe;EACbC,IAAI,EAAE,eADO;EAEbC,UAAU,EAAE;IACVL,OADU;IAEVE,OAFU;IAGVC;EAHU,CAFC;;EAObG,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,IADD;MAELC,IAAI,EAAE,EAFD;MAGLC,SAAS,EAAE,EAHN;MAILC,OAAO,EAAE;IAJJ,CAAP;EAMD,CAdY;;EAebC,OAAO,EAAE;IACPC,MAAM,GAAG;MACP,KAAK,KAAKC,GAAV,IAAiB,KAAKL,IAAtB,EAA4B;QAC1B,IAAIM,GAAE,GAAK,GAAE,KAAKJ,OAAQ,GAAE,KAAKG,GAAI,QAArC;MACF;;MACAZ,KAAI,CACDc,GADH,CACOD,GADP,EAEGE,IAFH,CAESC,GAAD,IAAS;QACb,KAAKR,SAAL,CAAeS,IAAf,CAAoBD,GAAG,CAACX,IAAxB;QACAa,OAAO,CAACC,GAAR,CAAY,KAAKX,SAAjB;MACD,CALH,EAMGY,KANH,CAMUC,KAAD,IAAW;QAChBH,OAAO,CAACC,GAAR,CAAYE,KAAZ;MACD,CARH;IASD,CAdM;;IAgBPC,WAAW,GAAG;MACZC,QAAQ,CAACC,cAAT,CAAwB,KAAxB,EAA+BC,KAA/B,GAAuC,EAAvC;IACD,CAlBM;;IAoBPC,SAAS,GAAG;MACV,KAAKnB,IAAL,CAAUU,IAAV,CAAe,KAAKX,IAApB;MACAY,OAAO,CAACC,GAAR,CAAY,KAAKZ,IAAjB;MACA,KAAKe,WAAL;IACD,CAxBM;;IA0BPK,SAAS,GAAG;MACV,KAAK,KAAKf,GAAV,IAAiB,KAAKL,IAAtB,EAA4B;QAC1B,KAAKA,IAAL,CAAUqB,MAAV,CAAiB,KAAKhB,GAAtB,EAA2B,CAA3B;MACF;;MACA,KAAK,KAAKiB,QAAV,IAAsB,KAAKrB,SAA3B,EAAsC;QACpC,KAAKA,SAAL,CAAeoB,MAAf,CAAsB,KAAKC,QAA3B,EAAqC,CAArC;MACF;IACD;;EAjCM;AAfI,CAAf","names":["BtnMenu","axios","CardCep","CardDash","name","components","data","info","ceps","enderecos","baseUrl","methods","getEnd","cep","url","get","then","res","push","console","log","catch","error","limparInput","document","getElementById","value","insertCep","removeCep","splice","endereco"],"sourceRoot":"","sources":["/home/samuel/programming/vue/teste-increazy/src/components/DashboardMenu.vue"],"sourcesContent":["<template>\n  <section class=\"card-dash\">\n    <div class=\"card-top\">\n      <div class=\"top-div-top\">\n        <p>\n          <input\n            type=\"text\"\n            v-model=\"info\"\n            class=\"input-cep\"\n            name=\"cep\"\n            id=\"cep\"\n            placeholder=\"Insira seu CEP\"\n          />\n        </p>\n        <p>\n          <BtnMenu icon=\"+\" msg=\"Adicionar endereço\" v-on:click=\"insertCep()\" />\n        </p>\n      </div>\n      <div class=\"top-div-middle\">\n        <div v-for=\"cep in ceps\" :key=\"cep\" class=\"div-cep\">\n          <CardCep :cep=\"cep\" />\n        </div>\n      </div>\n      <div class=\"top-div-down\">\n        <div>\n          <BtnMenu msg=\"Gerar endereço\" v-on:click=\"getEnd()\" />\n        </div>\n      </div>\n    </div>\n    <div\n      class=\"card-down\"\n      :id=\"endereco.siafi\"\n      v-for=\"endereco in enderecos\"\n      :key=\"endereco.siafi\"\n    >\n      <CardDash\n        :logradouro=\"endereco.logradouro\"\n        :cep=\"endereco.cep\"\n        :localidade=\"endereco.localidade\"\n        :uf=\"endereco.uf\"\n        v-on:click=\"removeCep()\"\n      />\n    </div>\n  </section>\n</template>\n<script>\nimport BtnMenu from \"./BtnMenu.vue\";\nimport axios from \"axios\";\nimport CardCep from \"./CardCep.vue\";\nimport CardDash from \"./CardDash.vue\";\nexport default {\n  name: \"DashboardMenu\",\n  components: {\n    BtnMenu,\n    CardCep,\n    CardDash,\n  },\n  data() {\n    return {\n      info: null,\n      ceps: [],\n      enderecos: [],\n      baseUrl: \"https://viacep.com.br/ws/\",\n    };\n  },\n  methods: {\n    getEnd() {\n      for (this.cep of this.ceps) {\n        var url = `${this.baseUrl}${this.cep}/json/`;\n      }\n      axios\n        .get(url)\n        .then((res) => {\n          this.enderecos.push(res.data);\n          console.log(this.enderecos);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n\n    limparInput() {\n      document.getElementById(\"cep\").value = \"\";\n    },\n\n    insertCep() {\n      this.ceps.push(this.info);\n      console.log(this.ceps);\n      this.limparInput()\n    },\n\n    removeCep() {\n      for (this.cep in this.ceps) {\n        this.ceps.splice(this.cep, 1);\n      }\n      for (this.endereco in this.enderecos) {\n        this.enderecos.splice(this.endereco, 1);\n      }\n    },\n  },\n};\n</script>\n<style scoped>\n.card-dash {\n  margin: 30px 135px;\n  min-height: 400px;\n  width: 540px;\n}\n\n.div-cep {\n  margin-bottom: 15px;\n}\n\n.card-top {\n  min-height: 220px;\n  width: 100%;\n  padding-bottom: 20px;\n  border-bottom: 1px solid #b7b7b7;\n}\n\n.top-div-top {\n  height: 60px;\n  width: 100%;\n  display: flex;\n  justify-content: space-between;\n}\n\n.top-div-middle {\n  min-height: 70px;\n  margin-top: 25px;\n  margin-bottom: 40px;\n  width: 100%;\n}\n\n.top-div-down {\n  width: 100%;\n  justify-content: end;\n  align-content: center;\n  display: flex;\n}\n\n.input-cep {\n  min-width: 250px;\n  height: 45px;\n  border: 2px solid #bbc4ce;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: flex-start;\n  padding: 16px;\n  box-sizing: border-box;\n  border-radius: 10px;\n}\n\n::-webkit-input-placeholder {\n  font-family: \"Open Sans\";\n  font-weight: 400;\n  size: 16px;\n}\n\n:-ms-input-placeholder {\n  font-family: \"Open Sans\";\n  font-weight: 400;\n  size: 16px;\n}\n</style>"]},"metadata":{},"sourceType":"module"}