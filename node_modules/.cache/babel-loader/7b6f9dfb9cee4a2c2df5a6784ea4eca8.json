  <section class=\"card-dash\">\n    <div class=\"card-top\">\n      <div class=\"top-div-top\">\n        <p>\n          <input\n            type=\"text\"\n            v-model=\"cep\"\n            class=\"input-cep\"\n            name=\"cep\"\n            id=\"cep\"\n            placeholder=\"Insira seu CEP\"\n          />\n        </p>\n        <p>\n          <BtnMenu icon=\"+\" msg=\"Adicionar endereço\" v-on:click=\"getCep()\" />\n        </p>\n      </div>\n      <div class=\"top-div-middle\">\n        <div\n          v-for=\"endereco in enderecos\"\n          :key=\"endereco.index\"\n          class=\"div-cep\"\n        >\n          <CardCep :cep=\"endereco.cep\" />\n        </div>\n      </div>\n      <div class=\"top-div-down\">\n        <div><BtnMenu msg=\"Gerar endereço\" v-on:click=\"renderCard()\"/></div>\n      </div>\n    </div>\n    <div class=\"card-down\" id=\"card-down\" v-for=\"endereco in enderecos\" :key=\"endereco.index\">\n      <CardDash\n        :logradouro=\"endereco.logradouro\"\n        :cep=\"endereco.cep\"\n        :localidade=\"endereco.localidade\"\n        :uf=\"endereco.uf\"\n      />\n    </div>\n  </section>\n</template>\n<script>\nimport BtnMenu from \"./BtnMenu.vue\";\nimport axios from \"axios\";\nimport CardCep from \"./CardCep.vue\";\nimport CardDash from \"./CardDash.vue\";\nexport default {\n  name: \"DashboardMenu\",\n  components: {\n    BtnMenu,\n    CardCep,\n    CardDash,\n  },\n  data() {\n    return {\n      enderecos: [],\n      baseUrl: \"https://viacep.com.br/ws/\",\n    };\n  },\n  methods: {\n    getCep() {\n      const url = `${this.baseUrl}${this.cep}/json/`;\n      axios\n        .get(url)\n        .then((res) => {\n          console.log(res.data); \n          this.enderecos.push(res.data);\n          console.log(this.enderecos);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n\n    renderCard() {\n        document.getElementById('card-down').style.display = \"block\";\n    }\n  },\n};\n</script>\n<style scoped>\n.card-dash {\n  margin: 30px 135px;\n  min-height: 400px;\n  width: 540px;\n}\n\n.card-down {\n    /* display: ; */\n}\n\n.div-cep {\n  margin-bottom: 15px;\n}\n\n.card-top {\n  min-height: 220px;\n  width: 100%;\n  padding-bottom: 20px;\n  border-bottom: 1px solid #b7b7b7;\n}\n\n.top-div-top {\n  height: 60px;\n  width: 100%;\n  display: flex;\n  justify-content: space-between;\n}\n\n.top-div-middle {\n  min-height: 70px;\n  margin-top: 25px;\n  margin-bottom: 40px;\n  width: 100%;\n}\n\n.top-div-down {\n  width: 100%;\n  display: flex;\n  justify-content: end;\n  align-content: center;\n}\n\n.input-cep {\n  min-width: 250px;\n  height: 45px;\n  border: 2px solid #bbc4ce;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: flex-start;\n  padding: 16px;\n  box-sizing: border-box;\n  border-radius: 10px;\n}\n\n::-webkit-input-placeholder {\n  font-family: \"Open Sans\";\n  font-weight: 400;\n  size: 16px;\n}\n\n:-ms-input-placeholder {\n  font-family: \"Open Sans\";\n  font-weight: 400;\n  size: 16px;\n}\n</style>"]},"metadata":{},"sourceType":"module"}