{"ast":null,"code":"import BtnMenu from \"./BtnMenu.vue\";\nimport axios from \"axios\";\nimport CardCep from \"./CardCep.vue\";\nimport CardDash from \"./CardDash.vue\";\nexport default {\n  name: \"DashboardMenu\",\n  components: {\n    BtnMenu,\n    CardCep,\n    CardDash\n  },\n\n  data() {\n    return {\n      cep: null,\n      baseUrl: \"https://viacep.com.br/ws/\",\n      ceps: [],\n      enderecos: []\n    };\n  },\n\n  methods: {\n    getCep() {\n      const url = `${this.baseUrl}${this.cep}/json/`;\n      axios.get(url).then(res => {\n        console.log(res.data);\n        this.data = JSON.stringify(res.data.cep);\n        this.dataEnd = JSON.stringify(res.data);\n        this.end = this.dataEnd.replace(/\"/g, \"\");\n        this.cep = this.data.replace(/\"/g, \"\");\n        this.ceps.push(this.cep);\n        this.datas = this.enderecos.push(this.end);\n        console.log(this.enderecos[0]);\n        console.log(this.ceps);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiCA,OAAOA,OAAP,MAAoB,eAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,OAAP,MAAoB,eAApB;AACA,OAAOC,QAAP,MAAqB,gBAArB;AACA,eAAe;EACbC,IAAI,EAAE,eADO;EAEbC,UAAU,EAAE;IACVL,OADU;IAEVE,OAFU;IAGVC;EAHU,CAFC;;EAObG,IAAI,GAAG;IACL,OAAO;MACLC,GAAG,EAAE,IADA;MAELC,OAAO,EAAE,2BAFJ;MAGLC,IAAI,EAAE,EAHD;MAILC,SAAS,EAAE;IAJN,CAAP;EAMD,CAdY;;EAebC,OAAO,EAAE;IACPC,MAAM,GAAG;MACP,MAAMC,GAAE,GAAK,GAAE,KAAKL,OAAQ,GAAE,KAAKD,GAAI,QAAvC;MACAN,KAAI,CACDa,GADH,CACOD,GADP,EAEGE,IAFH,CAESC,GAAD,IAAS;QACbC,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACV,IAAhB;QACA,KAAKA,IAAL,GAAYa,IAAI,CAACC,SAAL,CAAeJ,GAAG,CAACV,IAAJ,CAASC,GAAxB,CAAZ;QACA,KAAKc,OAAL,GAAeF,IAAI,CAACC,SAAL,CAAeJ,GAAG,CAACV,IAAnB,CAAf;QACA,KAAKgB,GAAL,GAAW,KAAKD,OAAL,CAAaE,OAAb,CAAqB,IAArB,EAA2B,EAA3B,CAAX;QACA,KAAKhB,GAAL,GAAW,KAAKD,IAAL,CAAUiB,OAAV,CAAkB,IAAlB,EAAwB,EAAxB,CAAX;QACA,KAAKd,IAAL,CAAUe,IAAV,CAAe,KAAKjB,GAApB;QACA,KAAKkB,KAAL,GAAa,KAAKf,SAAL,CAAec,IAAf,CAAoB,KAAKF,GAAzB,CAAb;QACAL,