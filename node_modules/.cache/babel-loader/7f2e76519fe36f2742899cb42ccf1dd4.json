ole.log(res.data);\n          this.data = JSON.stringify(res.data.cep);\n          this.dataEnd = JSON.stringify(res.data);\n          this.end = this.dataEnd.replace(/\"/g, \"\");\n          this.cep = this.data.replace(/\"/g, \"\");\n          this.ceps.push(this.cep);\n          this.datas = this.enderecos.push(this.end);\n          console.log(this.enderecos[0]);\n          console.log(this.ceps);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n};\n</script>\n<style scoped>\n.card-dash {\n  margin: 30px 135px;\n  min-height: 400px;\n  width: 540px;\n}\n\n.div-cep {\n  margin-bottom: 15px;\n}\n\n.card-top {\n  min-height: 220px;\n  width: 100%;\n  padding-bottom: 20px;\n  border-bottom: 1px solid #b7b7b7;\n}\n\n.top-div-top {\n  height: 60px;\n  width: 100%;\n  display: flex;\n  justify-content: space-between;\n}\n\n.top-div-middle {\n  min-height: 70px;\n  margin-top: 25px;\n  margin-bottom: 40px;\n  width: 100%;\n}\n\n.top-div-down {\n  width: 100%;\n  display: flex;\n  justify-content: end;\n  align-content: center;\n}\n\n.input-cep {\n  min-width: 250px;\n  height: 45px;\n  border: 2px solid #bbc4ce;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: flex-start;\n  padding: 16px;\n  box-sizing: border-box;\n  border-radius: 10px;\n}\n\n::-webkit-input-placeholder {\n  font-family: \"Open Sans\";\n  font-weight: 400;\n  size: 16px;\n}\n\n:-ms-input-placeholder {\n  font-family: \"Open Sans\";\n  font-weight: 400;\n  size: 16px;\n}\n</style>"]},"metadata":{},"sourceType":"module"}