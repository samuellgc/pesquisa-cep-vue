{"ast":null,"code":"import BtnMenu from \"./BtnMenu.vue\";\nimport axios from \"axios\";\nimport CardCep from \"./CardCep.vue\";\nimport CardDash from \"./CardDash.vue\";\nexport default {\n  name: \"DashboardMenu\",\n  components: {\n    BtnMenu,\n    CardCep,\n    CardDash\n  },\n\n  data() {\n    return {\n      cep: null,\n      uf: null,\n      localidade: null,\n      logradouro: null,\n      baseUrl: \"https://viacep.com.br/ws/\",\n      ceps: [],\n      logradouros: [],\n      ufs: [],\n      localidades: [],\n      endereco: null,\n      enderecos: []\n    };\n  },\n\n  methods: {\n    getCep() {\n      const url = `${this.baseUrl}${this.cep}/json/`;\n      axios.get(url).then(res => {\n        console.log(res.data);\n        this.data = JSON.stringify(res.data.cep);\n        this.dataUf = JSON.stringify(res.data.uf);\n        this.dataLocal = JSON.stringify(res.data.localidade);\n        this.dataLogra = JSON.stringify(res.data.logradouro);\n        this.uf = this.dataUf.replace(/\"/g, \"\");\n        this.local = this.dataLocal.replace(/\"/g, \"\");\n        this.logra = this.dataLogra.replace(/\"/g, \"\");\n        this.cep = this.data.replace(/\"/g, \"\");\n        this.ceps.push(this.cep);\n        this.ufs.push(this.uf);\n        this.localidades.push(this.local);\n        this.logradouros.push(this.logra);\n        console.log(this.ufs);\n        console.log(this.localidades);\n        console.log(this.ceps);\n        console.log(this.logradouros);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiCA,OAAOA,OAAP,MAAoB,eAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,OAAP,MAAoB,eAApB;AACA,OAAOC,QAAP,MAAqB,gBAArB;AACA,eAAe;EACbC,IAAI,EAAE,eADO;EAEbC,UAAU,EAAE;IACVL,OADU;IAEVE,OAFU;IAGVC;EAHU,CAFC;;EAObG,IAAI,GAAG;IACL,OAAO;MACLC,GAAG,EAAE,IADA;MAELC,EAAE,EAAE,IAFC;MAGLC,UAAU,EAAE,IAHP;MAILC,UAAU,EAAE,IAJP;MAKLC,OAAO,EAAE,2BALJ;MAMLC,IAAI,EAAE,EAND;MAOLC,WAAW,EAAE,EAPR;MAQLC,GAAG,EAAE,EARA